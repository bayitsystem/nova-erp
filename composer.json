{
    "name": "jrodas/module-generator",
    "description": "A Beautiful Laravel Nova tool helping modularize your resources.",
    "keywords": [
        "laravel",
        "nova",
        "erp",
        "crm",
        "nova-erp"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.3.15"
    },
    "autoload": {
        "psr-4": {}
    },
    "extra": {
        "laravel": {
            "providers": [
                "FalconERP\\Erp\\Providers\\ErpServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
